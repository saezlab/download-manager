[paths]
source =
    download_manager
    */site-packages/download-manager

[run]
branch = true
parallel = true
source = download_manager
omit = */__init__.py

[report]
exclude_lines =
    \#.*pragma:\s*no.?cover

    if __name__ == .__main__.

    ^\s*raise AssertionError\b
    ^\s*raise NotImplementedError\b
    ^\s*return NotImplemented\b
show_missing = true
precision = 2
